{
  "name": "payload-3-boilerplate",
  "version": "1.0.0",
  "description": "Payload V3 Website builder boilerplate for Railway",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "payload migrate && cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "seed": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/index.ts",
    "seed:production": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/production.ts",
    "seed:reset": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/reset.ts && tsx src/seed/index.ts",
    "create-admin": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/create-admin.ts",
    "generate:blog-post": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/generate-blog-post.ts",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-search": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dompurify": "^3.3.0",
    "express-rate-limit": "^8.1.0",
    "geist": "^1.3.1",
    "graphql": "^16.9.0",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.29.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "^0.378.0",
    "next": "^15.0.4",
    "nodemailer": "^7.0.9",
    "payload": "latest",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.4.0",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "validator": "^13.15.15",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.5.7",
    "@payloadcms/eslint-config": "^1.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/dompurify": "^3.0.5",
    "@types/escape-html": "^1.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "22.5.4",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@types/validator": "^13.15.3",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.20.6",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
